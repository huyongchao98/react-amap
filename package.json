{
  "name": "react-amap",
  "version": "1.2.9",
  "description": "AMap Component Based On React",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint . --quiet",
    "preinstall": "npx npm-force-resolutions",
    "browser-build": "rimraf dist && webpack && cross-env NODE_ENV=production webpack",
    "browser-build_d": "rimraf dist && webpack && cross-env NODE_ENV=development webpack",
    "npm-build": "rimraf lib && babel components -d lib",
    "npm-build_dev": "rimraf lib && babel components --watch -d lib --source-maps inline",
    "buildAll": "npm run browser-build && npm run npm-build",
    "buildAll_dev": "npm run browser-build_d && npm run npm-build_dev",
    "site": "npm run npm-build && bisheng build",
    "deploy": "npm run npm-build && bisheng gh-pages",
    "start": "npm run npm-build && bisheng start",
    "prepublish": "npm run buildAll",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "author": "ohislh@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elemefe/react-amap.git"
  },
  "keywords": [
    "map",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/ElemeFE/react-amap/issues"
  },
  "homepage": "https://github.com/ElemeFE/react-amap",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "antd": "4.12.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "bisheng": "3.0.0",
    "bisheng-plugin-antd": "1.3.2",
    "bisheng-plugin-react": "^1.1.2",
    "css-loader": "^5.0.2",
    "eslint": "7.20.0",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "flow-bin": "^0.144.0",
    "html-webpack-plugin": "5.1.0",
    "isomorphic-style-loader": "^5.1.0",
    "jsonml-to-react-component": "^0.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "react-router": "^5.2.0",
    "style-loader": "^2.0.0",
    "uglify-js": "^3.12.7",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "cross-env": "^7.0.3",
    "webpack-dev-server": "^3.11.2",
    "npm-force-resolutions": "0.0.3"
  },
  "resolutions": {
    "webpack": "^5.21.2"
  }
}

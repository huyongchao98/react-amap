{
  "name": "react-amap",
  "version": "1.2.9",
  "description": "AMap Component Based On React",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint . --quiet",
    "browser-build": "rm -rf dist && webpack && NODE_ENV=production webpack -p",
    "browser-build_d": "rm -rf dist && webpack && NODE_ENV=development webpack -d",
    "npm-build": "rm -rf lib && babel components -d lib",
    "npm-build_dev": "rm -rf lib && babel components --watch -d lib --source-maps inline",
    "buildAll": "npm run browser-build && npm run npm-build",
    "buildAll_dev": "npm run browser-build_d && npm run npm-build_dev",
    "site": "npm run npm-build && bisheng build",
    "deploy": "npm run npm-build && bisheng gh-pages",
    "start": "npm run npm-build && bisheng start",
    "prepublish": "if [ \"${NODE_ENV}\" = \"development\" ]; then  echo \"tests11\" \n npm run buildAll_dev; else echo \"tests22\" \n npm run buildAll; fi",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "author": "ohislh@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elemefe/react-amap.git"
  },
  "keywords": [
    "map",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/ElemeFE/react-amap/issues"
  },
  "homepage": "https://github.com/ElemeFE/react-amap",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "antd": "4.8.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "bisheng": "1.3.0",
    "bisheng-plugin-antd": "1.2.9",
    "bisheng-plugin-react": "^1.1.0",
    "css-loader": "^3.1.0",
    "eslint": "6.2.2",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-flowtype": "^3.12.2",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^4.0.0",
    "flow-bin": "^0.104.0",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-style-loader": "^5.1.0",
    "jsonml-to-react-component": "^0.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.0.1",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.6.0",
    "url-loader": "^2.0.0",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.7.2"
  }
}
